{
	"name": "vscode-seesaawiki",
	"displayName": "vscode-seesaawiki",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"main": "./dist/extension.js",
	"activationEvents": [
		"onCommand:vscode-seesaawiki.bold",
		"onCommand:vscode-seesaawiki.underline",
		"onCommand:vscode-seesaawiki.deleted"
	],
	"contributes": {
		"languages": [{
			"id": "seesaawiki",
			"aliases": ["Seesaa Wiki", "seesaawiki"],
			"extensions": ["seesaawiki"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "seesaawiki",
			"scopeName": "text.seesaawiki",
			"path": "./syntaxes/seesaawiki.tmLanguage.json"
		}],
		"snippets": [
			{
				"language": "seesaawiki",
				"path": "./snippets.json"
			}
		],
		"commands": [
			{
				"command": "vscode-seesaawiki.bold",
				"title": "Toggle bold",
				"when": "editorLangId == seesaawiki"
			},
			{
				"command": "vscode-seesaawiki.underline",
				"title": "Toggle underline",
				"when": "editorLangId == seesaawiki"
			},
			{
				"command": "vscode-seesaawiki.deleted",
				"title": "Toggle deleted",
				"when": "editorLangId == seesaawiki"
			}
		],
		"keybindings": [
			{
				"command": "vscode-seesaawiki.bold",
				"key": "ctrl+b"
			},
			{
				"command": "vscode-seesaawiki.underline",
				"key": "ctrl+u"
			},
			{
				"command": "vscode-seesaawiki.deleted",
				"key": "ctrl+d"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.75.0",
		"@types/glob": "^8.0.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"eslint": "^8.33.0",
		"glob": "^8.1.0",
		"mocha": "^10.1.0",
		"typescript": "^4.9.4",
		"ts-loader": "^9.4.2",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"@vscode/test-electron": "^2.2.2"
	},
	"license": "Sell LICENCE.md"
}
